<?xml version="1.0" encoding="UTF-8"?>
<jcr:root
    xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:cq="http://www.day.com/jcr/cq/1.0"
    xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Capgemini Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
  <content
      jcr:primaryType="nt:unstructured"
      sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
    <items jcr:primaryType="nt:unstructured">
      <column
          jcr:primaryType="nt:unstructured"
          sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
          <tabs
              jcr:primaryType="nt:unstructured"
              sling:resourceType="granite/ui/components/coral/foundation/tabs">
            <items jcr:primaryType="nt:unstructured">
              
              <!-- Properties Tab -->
              <propertiesTab
                  jcr:primaryType="nt:unstructured"
                  sling:resourceType="granite/ui/components/coral/foundation/container"
                  jcr:title="Properties">
                <items jcr:primaryType="nt:unstructured">
                  <mainContainer
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/coral/foundation/container">
                    <items jcr:primaryType="nt:unstructured">
                      <imageLeft
                          jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                          fieldLabel="Left Image"
                          name="./imageLeft"/>
                      <text1
                          jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                          fieldLabel="Text Field 1"
                          name="./text1"/>
                      <text2
                          jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                          fieldLabel="Text Field 2"
                          name="./text2"/>
                      <text3
                          jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                          fieldLabel="Text Field 3"
                          name="./text3"/>
                      <imageTop
                          jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                          fieldLabel="Top Image"
                          name="./imageTop"/>
                      <text4
                          jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                          fieldLabel="Text Field 4"
                          name="./text4"/>
                      <text5
                          jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                          fieldLabel="Text Field 5"
                          name="./text5"/>
                      <text6
                          jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                          fieldLabel="Text Field 6"
                          name="./text6"/>
                      <imageBottom
                          jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                          fieldLabel="Bottom Image"
                          name="./imageBottom"/>
                      <text7
                          jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                          fieldLabel="Text Field 7"
                          name="./text7"/>
                      <text8
                          jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                          fieldLabel="Text Field 8"
                          name="./text8"/>
                      <text9
                          jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                          fieldLabel="Text Field 9"
                          name="./text9"/>
                    </items>
                  </mainContainer>
                </items>
              </propertiesTab>
              
              <!-- Style Tab -->
              <styletab
                  jcr:primaryType="nt:unstructured"
                  sling:resourceType="granite/ui/components/coral/foundation/include"
                  path="/libs/cq/gui/components/authoring/dialog/style/tab"
                  jcr:title="style"/>
              
            </items>
          </tabs>
        </items>
      </column>
    </items>
  </content>
</jcr:root>