<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0">
    <!-- Component Folder Node (cq:Component) -->
    <cq:component jcr:primaryType="cq:Component">
        <!-- Define Dialog -->
        <cq:dialog jcr:primaryType="cq:Dialog">
            <items jcr:primaryType="nt:unstructured">
                <cq:field jcr:primaryType="nt:unstructured"
                          fieldLabel="Logo Path"
                          name="./logoPath"
                          allowed="[string]" />
            </items>
        </cq:dialog>
    </cq:component>
</jcr:root>
